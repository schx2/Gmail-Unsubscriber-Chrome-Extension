{
  "manifest_version": 2,
  "oauth2": {
    "client_id": "424205852045-sb591dpbqbaah5vbif8k63cu282t8hs2.apps.googleusercontent.com",
    "scopes": [
    "profile",
    "https://www.googleapis.com/auth/gmail.modify"
    ]
    },
    "name": "Unsubcribe from emails on Gmail",
    "short_name": "Gmail Unsubscriber",
    "version": "0.0.10",
    "content_security_policy": "object-src 'self';script-src 'self' https://*.google.com https://www.google-analytics.com;",
    "description": "Unsubscribe from emails, from your inbox",
    "permissions": [
    "background",
    "identity",
    "notifications",
    "storage",
    "tabs",
    "identity",
    "*://mail.google.com/*",
    "*://*.google.com/*",
    "https://*.googleusercontent.com/*"
    ],
    "web_accessible_resources": ["img/*.png"],
    "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "gmail.js"],
      "run_at": "document_end"
    }
    ],
    "background": {
      "scripts": ["background.js", "base64.js", "ga.js"],
      "persistent": false
    },
    "key": "",
    "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
    ,
    "browser_action": {
      "default_title": "Gmail unsubscriber stats",
      "default_icon": "img/icon128.png",
      "default_popup":"onboard.html"
    }
  }
